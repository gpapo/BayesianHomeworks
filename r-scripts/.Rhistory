library(compiler) # for matter of loop performance
envelop <- function(n, f, g, randomg, start, lower = 0, upper = 1 - lower, ...) {
result <- double(n)
optimization <- nlminb(objective = function(x) -f(x) / g(x), start, lower = lower, upper = upper, ...)
M <- -optimization$obj
ind <- 1
while (ind <= n) {
x <- randomg(1)
u <- runif(1, 0, M * g(x))
if (u < f(x)) {
result[ind] <- x
ind <- ind + 1
}
return(result)
}
